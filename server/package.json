        "apollo-server-express": "^2.19.2",
        "bcrypt": "^5.0.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.4",
        "mongoose": "^5.9.10",
        "socket.io": "^4.5.2"
    },
    "devDependencies": {
        "nodemon": "^2.0.3"
    }
}
